{
  "name": "angle-grinder",
  "license": "MIT",
  "version": "3.0.0",
  "scripts": {
    "build": "rimraf dist && yarn build:prod",
    "build:dev": "webpack --config config/webpack.config.js --mode=development --bail --profile --progress",
    "build:wip": "webpack --config config/webpack.config.js --mode=production --bail --profile --progress",
    "build:docs": "webpack --mode=production --bail --profile --progress --content-base ./docs",
    "build-ag-demo": "webpack --mode=production --bail --profile --progress --content-base ./examples/ag-demo-ui --pathout ./examples/ag-demo-grails/src/main/webapp",
    "build:app": "webpack --bail --profile --progress --mode=production",
    "clean": "rimraf dist && rimraf build",
    "test": "karma start tests/karma.conf.js --single-run --no-auto-watch --no-sandbox $*",
    "lint": "eslint src/",
    "lint-fix": "eslint src/ --fix",
    "start-docs": "webpack-dev-server --history-api-fallback --inline --foo=bar --mode=development --port 3000 --content-base ./docs",
    "start-sandbox": "webpack-dev-server --history-api-fallback --inline --progress --mode=development --port 3000 --content-base ./examples/sandbox",
    "start-ag-demo": "webpack-dev-server --history-api-fallback --inline --progress --mode=development --port 3000 --content-base ./examples/ag-demo-ui",
    "parcel": "parcel examples/simple/index.html",
    "parcel:build": "parcel build examples/simple/index.html"
  },
