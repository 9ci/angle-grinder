{
    "name": "sparkle",
    "version": "0.0.0",

    "dependencies": {
        "jqgrid": "basejump/jqGrid#v4.4.4-tweaks",
        "jquery": "1.9.1",
        "underscore": "~1.4.4",

        "bootstrap": "~2.3.1",
        "bootstrapx-clickover": "git://github.com/lecar-red/bootstrapx-clickover.git",
        "jquery-ui-bootstrap": "./custom_components/jquery-ui-bootstrap",
        "select2": "*",
        "select2-bootstrap-css": "t0m/select2-bootstrap-css",

        "angular": "~1.0.6",
        "angular-resource": "~1.0.6",
        "angular-ui-select2": "~0.0.2",
        "angular-bootstrap": "*"
    },

    "devDependencies": {
        "jasmine": "~1.3.1",
        "jasmine-jquery": "~1.5.3",
        "angular-mocks": "~1.0.7",
        "angular-scenario": "~1.0.7",

        "modernizr": "~2.6.2",
        "font-awesome": "*",
        "angular-ui": "*",
        "angular-strap": "~0.7.1"
    },

    "exportsOverride": {
